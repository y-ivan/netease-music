{
    "presets": [
        [
            "env", {
                "modules": false,
                "targets": {
                    "browers": ["> 1%", "last 2 versions", "not ie <= 8"]
                }
            }
        ],
        "stage-2"
    ],
    "plugins": [
        "transform-vue-jsx",
        "transform-runtime",
        ["import", {
            "libraryName": "mint-ui",
            "libraryDirectory": "es",
            "style": true
        }] 
    ]
}